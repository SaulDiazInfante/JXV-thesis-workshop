\mode<presentation>

%\setbeamercolor{structure}{fg=INRA@dinst,bg=INRA@dinst}
\setbeamercolor{structure}{fg=INRA@dinst}

\setbeamercolor{section in head/foot}{fg=white}
\setbeamercolor{headline}{fg=white,bg=INRA@dinst}
%
\setbeamercolor{alerted text}{fg=white!20!INRA@lali,bg=white}
\setbeamercolor{example text}{fg=white!20!INRA@dinst,bg=white}
\setbeamercolor{block text}{fg=white!20!INRA@lenv,bg=white}
%
\setbeamercolor{block title}{fg=INRA@lenv,bg=INRA@lenv!10!bg}
\setbeamercolor{block title alerted}{fg=INRA@lali,bg=INRA@lali!10!bg}
\setbeamercolor{block title example}{fg=INRA@dinst,bg=INRA@dinst!10!bg}

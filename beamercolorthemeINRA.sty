%Couleurs institutionnelles
\definecolor{INRA@linst}{RGB}{188,214,49}
\definecolor{INRA@dinst}{RGB}{139,172,33}
%Agriculture
\definecolor{INRA@lagr}{RGB}{244,125,32} % plus ou moins orange
%\definecolor{INRA@dagr}{RGB}{109,70,53}
%%Environnement
\definecolor{INRA@lenv}{RGB}{42,162,224} % entre bleu et azur
%\definecolor{INRA@denv}{RGB}{1,63,78}
%%Alimentation
\definecolor{INRA@lali}{RGB}{237,26,92} % entre rose et violet
%\definecolor{INRA@lali}{RGB}{123,41,70}
